{
	"schemaVersion": 1,
	"id": "player-finder",
	"version": "${version}",
	"name": "Player Finder",
	"description": "Client-side Fabric HUD mod that displays nearby player information directly on your screen in Minecraft.",
	"authors": [
		"Wenhan"
	],
	"environment": "client",
	"entrypoints": {
		"client": [
			"com.example.playerfinder.PlayerFinderMod"
		]
	},
	"mixins": [
		"player-finder.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.18.0",
		"fabric-api": "*",
		"minecraft": "1.21.11"
	}
}
